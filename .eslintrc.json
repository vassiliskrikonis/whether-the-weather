{
  "env": {
    "browser": true,
    "es6": true,
    "cypress/globals": true
  },
  "extends": ["eslint:recommended", "plugin:cypress/recommended", "plugin:react/recommended"],
  "plugins": ["cypress", "react"],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "jsx": true
  },
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly",
    "process": "readonly"
  },
  "rules": {
    "no-console": "off",
    "react/prop-types": 1
  }
}
